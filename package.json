{
  "name": "vue-wechat-login",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "commit": "git-cz",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "lint:eslint": "eslint --cache --fix \"**/*.{js,cjs,ts,vue}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{css,less,vue}\"",
    "lint:prettier": "prettier --write \"**/*.{js,cjs,ts,vue,css,less}\"",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "**/*.{js,cjs,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{css,less,vue}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "good-storage": "^1.1.1",
    "js-cookie": "^3.0.5",
    "numeral": "^2.0.6",
    "vant": "^4.9.5",
    "vue": "^3.5.3",
    "vue-router": "^4.4.3",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.10.0",
    "@types/good-storage": "^1.1.0",
    "@types/js-cookie": "^3.0.3",
    "@types/numeral": "^2.0.2",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/compiler-sfc": "^3.5.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "commitizen": "^4.3.0",
    "consola": "^3.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "less": "^4.1.3",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.45",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-px-to-viewport-8-plugin": "^1.2.2",
    "prettier": "^3.3.3",
    "qs": "^6.13.0",
    "stylelint": "^16.9.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-less": "^3.0.1",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^5.0.2",
    "typescript": "^5.6.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.20.5"
  }
}
