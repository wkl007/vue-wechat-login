    ],
    "src/**/*.{vue,htm,html,css,sss,less,scss}": [
      "stylelint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dayjs": "^1.11.7",
    "good-storage": "^1.1.1",
    "js-cookie": "^3.0.5",
