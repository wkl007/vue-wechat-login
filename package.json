{
  "name": "vue-wechat-login",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "commit": "git-cz",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "lint:eslint": "eslint --cache --fix \"**/*.{js,cjs,ts,vue}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{css,less,vue}\"",
    "lint:prettier": "prettier --write \"**/*.{js,cjs,ts,vue,css,less}\"",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "**/*.{js,cjs,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{css,less,vue}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "good-storage": "^1.1.1",
    "js-cookie": "^3.0.5",
    "numeral": "^2.0.6",
    "vant": "^4.9.1",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.6.0",
    "@types/good-storage": "^1.1.0",
    "@types/js-cookie": "^3.0.3",
    "@types/numeral": "^2.0.2",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/compiler-sfc": "^3.4.31",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "commitizen": "^4.3.0",
    "consola": "^3.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "less": "^4.1.3",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-px-to-viewport-8-plugin": "^1.2.2",
    "prettier": "^3.3.2",
    "qs": "^6.12.3",
    "stylelint": "^16.6.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-less": "^3.0.1",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.5.3",
    "unplugin-vue-components": "^0.27.2",
    "vite": "^5.3.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.20.0"
  }
}
